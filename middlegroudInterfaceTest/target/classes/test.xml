<?xml version="1.0" encoding="utf-8" ?>


<suite name="好食期接口测试">
    <test name="好食期接口测试用例(openapi)">
        <classes>

            <class name="com.duoweidu.cases.hsq.openapi.user_login_test">
                <methods>
                    <include name="login_true"/>
                </methods>
            </class>
            <!--添加购物车-->
            <class name="com.duoweidu.cases.hsq.openapi.user_addskutocartv1_test">
                <methods>
                    <include name="user_addskutocartv1_true"/>
                    <include name="user_addskutocartv1_true2"/>
                    <include name="userAddskutocartv1Merchant"/>
                </methods>
            </class>
            <!--编辑购物车-->
            <class name="com.duoweidu.cases.hsq.openapi.UserEditcartsku">
                <methods>
                    <include name="userEditcartsku"/>
                </methods>
            </class>
            <class name="com.duoweidu.cases.hsq.openapi.OrderOderinitv1">
                <methods>
                    <include name="orderOderinitv1"></include>
                    <include name="orderOderinitv1Merchant"></include>
                </methods>
            </class>
            <class name="com.duoweidu.cases.hsq.openapi.OrderSubmitorderv1">
                <methods>
                    <include name="orderSubmitorderv1"></include>
                    <include name="orderSubmitorderv1Merchant"></include>
                </methods>
            </class>
        </classes>
    </test>


    <listeners>

        <listener class-name="com.duoweidu.config.ExtentTestNGIReporterListener"/>
    </listeners>

</suite>